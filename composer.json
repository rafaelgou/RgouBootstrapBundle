{
    "name": "rgou/bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Twitter Bootstrap Symfony2 bundle with generator for Doctrine ORM and ODM",
    "keywords": ["symfony", "bundle", "twitter", "bootstrap", "theme"],
    "homepage": "http://github.com/rafaelgou/RgouBoostrapBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Goulart",
            "email": "rafaelgou@gmail.com",
            "homepage": "http://tech.rgou.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "knplabs/knp-paginator-bundle": "2.3.*@dev",
        "knplabs/knp-menu-bundle": "*",
        "sonata-project/intl-bundle": "2.2.*@dev"
    },
    "require-dev": {
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev"        
    },
    "autoload": {
        "psr-0": { "Rgou\\BootstrapBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Rgou/BootstrapBundle",
    "repositories": [
        {
            "type":"vcs",
            "url":"/var/www/citydata/Portal/bundles/SonataIntlBundle"
        },
        {
            "type":"vcs",
            "url":"/var/www/citydata/Portal/bundles/KnpMenu"
        },
        {
            "type":"vcs",
            "url":"/var/www/citydata/Portal/bundles/KnpMenuBundle"
        }
    ]
}
